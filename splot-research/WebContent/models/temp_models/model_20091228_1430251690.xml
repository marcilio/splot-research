<feature_model name="Testing">
<meta>
<data name="description"></data>
<data name="creator"></data>
<data name="address"></data>
<data name="email"></data>
<data name="phone"></data>
<data name="website"></data>
<data name="organization"></data>
<data name="department"></data>
<data name="date"></data>
<data name="reference"></data>
</meta>
<feature_tree>
:r Testing(_r)
	:m Feature-1(_r_1)
		:o Feature-2(_r_1_2)
			:o Feature-3(_r_1_2_3)
</feature_tree>
<constraints>
constraint_1:_r_1 or ~_r_1_2_3
constraint_2:_r_1 or ~_r_1_2
</constraints>
</feature_model>
