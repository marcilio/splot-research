<feature_model name="Product">
<meta>
<data name="description"></data>
<data name="creator"></data>
<data name="address"></data>
<data name="email"></data>
<data name="phone"></data>
<data name="website"></data>
<data name="organization"></data>
<data name="department"></data>
<data name="date"></data>
<data name="reference"></data>
</meta>
<feature_tree>
:r Product(_r)
	:g (_r_2) [1,1] 
		: V1-0(_r_2_3)
		: V2-0(_r_2_4)
	:g (_r_5) [1,1] 
		: Black(_r_5_6)
		: White(_r_5_7)
		: Blue(_r_5_8)
		: Red(_r_5_9)
		: Green(_r_5_10)
</feature_tree>
<constraints>
constraint_1:~_r_2_3 or _r_5_6 or _r_5_7
</constraints>
</feature_model>
