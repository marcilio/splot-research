<!-- This model was created online using SPLOT's Feature Model Editor (http://www.splot-research.org) on Tue, Nov 17, 2009 - 2:27 PM  -->
<feature_model name="Car">
<meta>
<data name="description"></data>
<data name="creator"></data>
<data name="address"></data>
<data name="email"></data>
<data name="phone"></data>
<data name="website"></data>
<data name="organization"></data>
<data name="department"></data>
<data name="date"></data>
<data name="reference"></data>
</meta>
<feature_tree>
:r Car(_r)
	:m Engine(_r_1)
		:g (_r_1_2) [1,1] 
			: 3.0(_r_1_2_3)
			: 2.0(_r_1_2_4)
	:o Air bag(_r_5)
		:o Number(_r_5_6)
</feature_tree>
<constraints>
constraint_1:~_r_1_2_3 or _r_5
constraint_2:~_r_5 or _r_5_6
</constraints>
</feature_model>
